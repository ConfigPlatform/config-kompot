{
  "pages": [
    {
      "path": "/invoice",
      "content": [
        {
          "type": "text",
          "value": "Invoices"
        },
        {
          "type": "button",
          "text": "Add Invoice",
          "serverHandler": "open_invoice_create_sidepanel"
        },
        {
          "type": "table",
          "id": "invoice_table",
          "className": "min-w-full leading-normal",
          "select": "invoice_get_all",
          "columns": [
            {
              "type": "column",
              "className": "px-5 py-3 border-b-2 border-gray-200 bg-gray-100 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider",
              "name": "ID",
              "cell": {
                "className": "px-5 py-5 border-b border-gray-200 bg-white text-sm",
                "content": [
                  {
                    "type": "text",
                    "value": "$id",
                    "vars": {
                      "id": "$id"
                    }
                  }
                ]
              }
            },
            {
              "type": "column",
              "className": "px-5 py-3 border-b-2 border-gray-200 bg-gray-100 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider",
              "name": "Client",
              "cell": {
                "className": "px-5 py-5 border-b border-gray-200 bg-white text-sm",
                "content": [
                  {
                    "type": "text",
                    "value": "$name",
                    "vars": {
                      "client.lastName": "$name"
                    }
                  }
                ]
              }
            },
            {
              "type": "column",
              "className": "px-5 py-3 border-b-2 border-gray-200 bg-gray-100 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider",
              "name": "Products",
              "cell": {
                "className": "px-5 py-5 border-b border-gray-200 bg-white text-sm",
                "content": [
                  {
                    "type": "list",
                    "data": "products",
                    "separator": ", ",
                    "content": [
                      {
                        "type": "text",
                        "value": "$name",
                        "vars": {
                          "name": "$name"
                        }
                      }
                    ]
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "pagination",
          "select": "invoice_get_all"
        }
      ]
    },
    {
      "path": "/client",
      "content": [
        {
          "type": "text",
          "value": "Clients",
          "className": "text-3xl text-gray-900 font-medium"
        },
        {
          "type": "row",
          "className": "",
          "content": [
            {
              "type": "col",
              "width": "8",
              "className": "",
              "content": [
                {
                  "type": "filter",
                  "placeholder": "Enter first name",
                  "filterBy": "firstName",
                  "select": "client_get_all"
                },
                {
                  "type": "filter",
                  "placeholder": "Enter last name",
                  "filterBy": "lastName",
                  "select": "client_get_all"
                },
                {
                  "type": "filter",
                  "placeholder": "Enter phone",
                  "filterBy": "phone",
                  "select": "client_get_all"
                },
                {
                  "type": "button",
                  "text": "Add Client",
                  "serverHandler": "open_client_create_sidepanel"
                }
              ]
            },
            {
              "type": "col",
              "className": "flex justify-end",
              "width": "4",
              "content": [
                {
                  "type": "button",
                  "content": [
                    {
                      "type": "text",
                      "value": "Add Client"
                    }
                  ],
                  "className": "btn btn-primary btn btn-primary px-3 py-2 mx-2 text-sm text-white duration-150 cursor-pointer bg-blue-700 rounded hover:bg-blue-900 active:shadow-lg",
                  "serverHandler": "open_client_create_sidepanel"
                }
              ]
            }
          ]
        },

        {
          "type": "table",
          "id": "client_table",
          "className": "min-w-full leading-normal",
          "select": "client_get_all",
          "columns": [
            {
              "type": "container",
              "className": "",
              "name": "ID",
              "sortBy": "id",
              "cell": {
                "className": "",
                "content": [
                  {
                    "type": "text",
                    "value": "$id",
                    "vars": {
                      "id": "$id"
                    }
                  }
                ]
              }
            },
            {
              "type": "container",
              "className": "",
              "name": "First Name",
              "cell": {
                "className": "",
                "content": [
                  {
                    "type": "text",
                    "value": "$firstName",
                    "vars": {
                      "firstName": "$firstName"
                    }
                  }
                ]
              }
            },
            {
              "type": "container",
              "className": "",
              "name": "Last Name",
              "sortBy": "lastName",
              "cell": {
                "className": "",
                "content": [
                  {
                    "type": "text",
                    "value": "$lastName",
                    "vars": {
                      "lastName": "$lastName"
                    }
                  }
                ]
              }
            },
            {
              "type": "container",
              "className": "",
              "name": "Phone",
              "sortBy": "phone",
              "cell": {
                "className": "",
                "content": [
                  {
                    "type": "text",
                    "value": "$phone",
                    "vars": {
                      "phone": "$phone"
                    }
                  }
                ]
              }
            },
            {
              "type": "container",
              "className": "",
              "name": "Invoices",
              "cell": {
                "className": "",
                "content": [
                  {
                    "type": "list",
                    "data": "invoices",
                    "separator": ", ",
                    "content": [
                      {
                        "type": "text",
                        "value": "$id",
                        "vars": {
                          "id": "$id"
                        }
                      }
                    ]
                  }
                ]
              }
            },
            {
              "type": "container",
              "className": "",
              "name": "Controls",
              "cell": {
                "className": "",
                "content": [
                  {
                    "type": "dropdown",
                    "width": "80px",
                    "button": {
                      "content": [
                        {
                          "type": "svg",
                          "element": "<svg class=\"w-5 h-5\" aria-hidden=\"true\" xmlns=\"http://www.w3.org/2000/svg\" fill=\"currentColor\" viewBox=\"0 0 4 15\">\n<path d=\"M3.5 1.5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0Zm0 6.041a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0Zm0 5.959a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0Z\"/>\n</svg>"
                        }
                      }
                    ]
                  }
                },
                {
                  "type": "column",
                  "className": "px-5 py-3 border-b-2 border-gray-200 bg-gray-100 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider",
                  "name": "Phone",
                  "sortBy": "phone",
                  "cell": {
                    "className": "px-5 py-5 border-b border-gray-200 bg-white text-sm",
                    "content": [
                      {
                        "type": "text",
                        "value": "$phone",
                        "vars": {
                          "phone": "$phone"
                        }
                      }
                    ]
                  }
                },
                {
                  "type": "column",
                  "className": "px-5 py-3 border-b-2 border-gray-200 bg-gray-100 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider",
                  "name": "Invoices",
                  "cell": {
                    "className": "px-5 py-5 border-b border-gray-200 bg-white text-sm",
                    "content": [
                      {
                        "type": "list",
                        "data": "invoices",
                        "separator": ", ",
                        "content": [
                          {
                            "type": "text",
                            "value": "$id",
                            "vars": {
                              "id": "$id"
                            }
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "type": "column",
                  "className": "px-5 py-3 border-b-2 border-gray-200 bg-gray-100 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider",
                  "name": "Controls",
                  "cell": {
                    "className": "px-5 py-5 border-b border-gray-200 bg-white text-sm",
                    "content": [
                      {
                        "type": "dropdown",
                        "width": "80px",
                        "button": {
                          "content": [
                            {
                              "type": "svg",
                              "element": "<svg class=\"w-5 h-5\" aria-hidden=\"true\" xmlns=\"http://www.w3.org/2000/svg\" fill=\"currentColor\" viewBox=\"0 0 4 15\">\n<path d=\"M3.5 1.5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0Zm0 6.041a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0Zm0 5.959a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0Z\"/>\n</svg>"
                            }
                          ]
                        },
                        "action": "click",
                        "options": [
                          {
                            "type": "button",
                            "text": "Edit"
                          },
                          {
                            "type": "button",
                            "data": {
                              "id": "$id"
                            },
                            "serverHandler": "client_delete_one",
                            "text": "Delete"
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "pagination",
              "select": "client_get_all"
            }
          ]
        }
      ]
    },
    {
      "path": "/product",
      "content": [
        {
          "type": "text",
          "value": "Products"
        },
        {
          "type": "button",
          "text": "Add Product",
          "serverHandler": "open_product_create_sidepanel"
        },
        {
          "type": "table",
          "id": "product_table",
          "className": "min-w-full leading-normal",
          "select": "product_get_all",
          "columns": [
            {
              "type": "column",
              "className": "px-5 py-3 border-b-2 border-gray-200 bg-gray-100 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider",
              "name": "ID",
              "cell": {
                "className": "px-5 py-5 border-b border-gray-200 bg-white text-sm",
                "content": [
                  {
                    "type": "text",
                    "value": "$id",
                    "vars": {
                      "id": "$id"
                    }
                  }
                ]
              }
            },
            {
              "type": "column",
              "className": "px-5 py-3 border-b-2 border-gray-200 bg-gray-100 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider",
              "name": "Name",
              "cell": {
                "className": "px-5 py-5 border-b border-gray-200 bg-white text-sm",
                "content": [
                  {
                    "type": "text",
                    "value": "$name",
                    "vars": {
                      "name": "$name"
                    }
                  }
                ]
              }
            },
            {
              "type": "column",
              "className": "px-5 py-3 border-b-2 border-gray-200 bg-gray-100 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider",
              "name": "Price",
              "cell": {
                "className": "px-5 py-5 border-b border-gray-200 bg-white text-sm",
                "content": [
                  {
                    "type": "text",
                    "value": "$$price",
                    "vars": {
                      "price": "$price"
                    }
                  }
                ]
              }
            },
            {
              "type": "column",
              "className": "px-5 py-3 border-b-2 border-gray-200 bg-gray-100 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider",
              "name": "Description",
              "cell": {
                "className": "px-5 py-5 border-b border-gray-200 bg-white text-sm",
                "content": [
                  {
                    "type": "text",
                    "value": "$description",
                    "vars": {
                      "description": "$description"
                    }
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "pagination",
          "select": "product_get_all"
        }
      ]
    }
  ]
}
